module compare (
    input z,  // output z from adder
    input v,  // output v from adder
    input n,  // output n from adder
    input alufn[6], // ALUFN[5:0]
    output cmp[16] // compare output
  ) {
sig cmpLast;
  always {
    case(alufn[3:0]){
      4b0011:
        cmpLast = z;
      4b0101:
        cmpLast = n^v;   
      4b0111:
        cmpLast= z|(n^v);
      
      default:
        cmpLast = b0;
       }
    cmp[0] = cmpLast;
    cmp[15:1] = 15b00000000000000;
    }
}