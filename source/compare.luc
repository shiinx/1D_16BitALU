module compare (
    input z,  // output z from adder
    input v,  // output v from adder
    input n,  // output n from adder
    input alufn[6], 
    output cmp[16]
  ) {
sig cmpLast;
  always {
    case(alufn[2:1]){
      b01:
        cmpLast = z ;
      b10:
        cmpLast = n^v;   
      b11:
        cmpLast= z|(n^v);
      
      default:
        cmpLast = b0;
       }
    cmp[15] = cmpLast;
    cmp[14:0] = 15b00000000000000;
    }
}